{
  "name": "ambisonics",
  "version": "0.3.0",
  "description": "web audio classes for Ambisonic processing",
  "author": "Archontis Politis <archontis.politis@aalto.com>",
  "contributors": [
    {
      "name": "David Poirier-Quinot",
      "email": "David.Poirier-Quinot@ircam.fr"
    }
  ],
  "type": "module",
  "main": "./dist/ambisonics.umd.js",
  "module": "./dist/ambisonics.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ambisonics.es.js",
      "require": "./dist/ambisonics.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite.config.min.ts",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/polarch/JSAmbisonics.git"
  },
  "keywords": [
    "audio",
    "spatial",
    "binaural",
    "ambisonics"
  ],
  "dependencies": {
    "convex-hull": "^1.0.3",
    "get-float-time-domain-data": "^0.1.0",
    "numeric": "^1.2.6",
    "serve-sofa-hrir": "Ircam-RnD/serveSofaHrir",
    "spherical-harmonic-transform": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "happy-dom": "^20.0.11",
    "typescript": "^5.9.3",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.53.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "src/"
  ]
}
